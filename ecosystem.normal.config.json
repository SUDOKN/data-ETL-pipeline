{
  "apps": [
    {
      "name": "extract-q-bot",
      "script": "./data_etl_app/src/data_etl_app/bots/new_extract_queue_bot.py",
      "interpreter": "./.venv/bin/python3",
      "args": "--priority 0 --max_concurrent_manufacturers 25 --debug INFO",
      "cwd": "./",
      "instances": 1,
      "autorestart": true,
      "watch": false,
      "max_memory_restart": "1G",
      "log_date_format": "YYYY-MM-DD HH:mm:ss Z",
      "error_file": "./logs/extract-queue-bot-normal-error.log",
      "out_file": "./logs/extract-queue-bot-normal-out.log",
      "log_file": "./logs/extract-queue-bot-normal-combined.log",
      "time": true,
      "kill_timeout": 2000,
      "shutdown_with_message": true,
      "listen_timeout": 8000,
      "wait_ready": false,
      "max_restarts": 5,
      "min_uptime": "10s",
      "restart_delay": 5000,
      "env": {
        "PYTHONPATH": "./data_etl_app/src:./core/src:./open_ai_key_app/src",
        "PYTHONUNBUFFERED": "1",
        "VIRTUAL_ENV": "./.venv",
        "PATH": "./.venv/bin:/usr/local/bin:/usr/bin:/bin"
      }
    },
    {
      "name": "scrape-q-bot",
      "script": "./scraper_app/src/scraper_app/bots/new_scrape_queue_bot.py",
      "interpreter": "./.venv/bin/python3",
      "args": "--priority 0 --max_concurrent_browsers 5 --max_depth 5 --scrape_timeout 60",
      "cwd": "./",
      "instances": 1,
      "autorestart": true,
      "watch": false,
      "max_memory_restart": "2G",
      "log_date_format": "YYYY-MM-DD HH:mm:ss Z",
      "error_file": "./logs/scrape-queue-bot-normal-error.log",
      "out_file": "./logs/scrape-queue-bot-normal-out.log",
      "log_file": "./logs/scrape-queue-bot-normal-combined.log",
      "time": true,
      "kill_timeout": 2000,
      "shutdown_with_message": true,
      "listen_timeout": 8000,
      "wait_ready": false,
      "max_restarts": 5,
      "min_uptime": "10s",
      "restart_delay": 5000,
      "env": {
        "PYTHONPATH": "./scraper_app/src:./core/src:./open_ai_key_app/src",
        "PYTHONUNBUFFERED": "1",
        "VIRTUAL_ENV": "./.venv",
        "PATH": "./.venv/bin:/usr/local/bin:/usr/bin:/bin"
      }
    },
    {
      "name": "data-etl-api",
      "script": "./.venv/bin/python3",
      "interpreter": "none",
      "args": "-m gunicorn data_etl_app.main:app -w 1 -k uvicorn.workers.UvicornWorker --bind 0.0.0.0:8000 --timeout 120 --max-requests 1000 --preload --log-level info",
      "cwd": "./",
      "instances": 1,
      "autorestart": true,
      "watch": false,
      "max_memory_restart": "1G",
      "log_date_format": "YYYY-MM-DD HH:mm:ss Z",
      "error_file": "./logs/data-etl-api-error.log",
      "out_file": "./logs/data-etl-api-out.log",
      "log_file": "./logs/data-etl-api-combined.log",
      "time": true,
      "kill_timeout": 1000,
      "shutdown_with_message": true,
      "listen_timeout": 8000,
      "wait_ready": false,
      "max_restarts": 5,
      "min_uptime": "10s",
      "restart_delay": 5000,
      "env": {
        "PYTHONPATH": "./data_etl_app/src:./core/src:./open_ai_key_app/src",
        "PYTHONUNBUFFERED": "1",
        "NODE_ENV": "production",
        "LOG_LEVEL": "INFO",
        "VIRTUAL_ENV": "./.venv",
        "PATH": "./.venv/bin:/usr/local/bin:/usr/bin:/bin"
      }
    },
    {
      "name": "create-deferred-mfg",
      "script": "./open_ai_key_app/src/open_ai_key_app/scripts/create_deferred_manufacturers.py",
      "interpreter": "./.venv/bin/python3",
      "args": "--mode all --max-concurrent 50 --stats-interval 50 --yes",
      "cwd": "./",
      "instances": 1,
      "autorestart": true,
      "watch": false,
      "max_memory_restart": "4G",
      "log_date_format": "YYYY-MM-DD HH:mm:ss Z",
      "error_file": "./logs/create-deferred-mfg-error.log",
      "out_file": "./logs/create-deferred-mfg-out.log",
      "log_file": "./logs/create-deferred-mfg-combined.log",
      "time": true,
      "kill_timeout": 3000,
      "shutdown_with_message": false,
      "listen_timeout": 8000,
      "wait_ready": false,
      "max_restarts": 10,
      "min_uptime": "60s",
      "restart_delay": 10000,
      "env": {
        "PYTHONPATH": "./data_etl_app/src:./core/src:./open_ai_key_app/src:./scraper_app/src",
        "PYTHONUNBUFFERED": "1",
        "VIRTUAL_ENV": "./.venv",
        "PATH": "./.venv/bin:/usr/local/bin:/usr/bin:/bin"
      }
    }
  ]
}
